-- Scripts:

local function PIUEA_fake_script() -- vis.LocalScript 
	local script = Instance.new('LocalScript', vis)

	local sensitivity = 4 -- backwards (lower = more sensitive)
	local randomness = 2 -- how randomly it moves
	local smoothness = 0.07 -- how smoothed the movement it
	
	wait(4)
	function update() -- will spawn in the background later
		for i,v in pairs(script.Parent:GetChildren()) do -- gets all instances in parent
			local sound = Music -- the sound instance to visualize
			if v.Name == "Bar" then -- if it is a visualizer bar
				local function updatebar()
					while wait() do -- repeats
						local voll = sound.PlaybackLoudness / sensitivity
						v:TweenSize(UDim2.new(0,24,0,voll/math.random(1,randomness)),Enum.EasingDirection.InOut,Enum.EasingStyle.Quad,smoothness)
						
					end
				end
				spawn(updatebar)
			end
		end
	end
	
	spawn(update)
end
coroutine.wrap(PIUEA_fake_script)()
end

