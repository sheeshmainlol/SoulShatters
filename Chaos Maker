pa2.Name = "weld2"
pa2.Part0 = game.Players.LocalPlayer.Character.Bracelets.Bracelet1.Outer
pa2.Part1 = game.Players.LocalPlayer.Character["Left Arm"]
pa2.C0 = cf0:Inverse()

